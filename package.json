{
  "name": "gif-maker-client",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "concurrently \"npm run start_server\" \"npm run start_client\"",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "gen-stats": "ng build --prod --stats-json",
    "view-stats": "webpack-bundle-analyzer dist/stats.json",
    "start_client": "ng serve --proxy-config proxy.conf.json",
    "start_server": "nodemon -x node -r dotenv/config server.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.8",
    "@angular/common": "~7.2.8",
    "@angular/compiler": "~7.2.8",
    "@angular/core": "~7.2.8",
    "@angular/forms": "~7.2.8",
    "@angular/http": "~7.2.8",
    "@angular/platform-browser": "~7.2.8",
    "@angular/platform-browser-dynamic": "~7.2.8",
    "@angular/router": "~7.2.8",
    "core-js": "^2.5.4",
    "express": "^4.16.4",
    "gifshot": "^0.4.5",
    "rxjs": "~6.4.0",
    "ytdl-core": "^0.29.1",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.5",
    "@angular/cli": "~7.0.6",
    "@angular/compiler-cli": "^7.2.8",
    "@angular/language-service": "~7.2.8",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "concurrently": "^4.1.0",
    "dotenv": "^6.2.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "nodemon": "^1.18.10",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.4",
    "webpack-bundle-analyzer": "^3.1.0"
  }
